{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "Server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "Server/index.js" },
      { "src": "/(.*)", "dest": "client/build/$1" }
    ]
  }
  